{
  "hosting": {
    "public": "site/build/dist/js/productionExecutable/public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "!/@(*.js|*.css|*.json|*.png|*.jpg|*.jpeg|*.gif|*.svg|*.ico|*.woff|*.woff2|*.ttf|*.eot|*.map)",
        "destination": "/index.html"
      }
    ]
  }
}